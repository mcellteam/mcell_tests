begin parameters
  # provide diffusion constant for used species 
  MCELL_DIFFUSION_CONSTANT_2D_A 1.0e-8
  MCELL_DIFFUSION_CONSTANT_2D_B 2.0e-8
  MCELL_DIFFUSION_CONSTANT_2D_C 1.3e-8  
end parameters

begin compartments 
  PM 2 0
  CP 3 1/8 PM           # 3D (volume) compartment with volume 1um^3
end compartments

begin seed species
  A@PM  100        # release 100 of A and 100 of B in compartment CP 
  B@PM  100
end seed species

begin reaction rules
  A + B -> C 100   # rate set so that no rxns are missed
end reaction rules

begin observables 
  Species A A
end observables 
  